{
  "name": "AggregatorJS",
  "version": "1.0.0",
  "private": true,
  "scripts": {    
    "buildApp": "babel ./application/src --out-dir ./application/dist --source-maps",
    "buildSys": "babel ./system/src --out-dir ./system/dist --source-maps",    
    "dev":"pm2 restart pm2.config.js --only AggregatorJS_dev",
    "test": "pm2 restart pm2.config.js --only AggregatorJS_test",
    "sim": "pm2 restart pm2.config.js --only AggregatorJS_sim",
    "prod": "pm2 restart pm2.config.js --only AggregatorJS_prod",
    "stopDev": "pm2 stop AggregatorJS_dev" ,
    "stopTest": "pm2 stop AggregatorJS_test" ,
    "stopSim": "pm2 stop AggregatorJS_sim" ,
    "stopProd": "pm2 stop AggregatorJS_prod"
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "captchapng": "0.0.1",
    "connect-redis": "^3.1.0",
    "cookie-parser": "~1.4.3",
    "cross-env": "^3.1.3",
    "debug": "~2.2.0",
    "ejs": "~2.4.1",
    "express": "~4.13.4",
    "express-session": "^1.14.1",
    "require-directory": "^2.1.1",
    "serve-favicon": "~2.3.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "log4js": "^1.0.1",
    "object-mapper": "^3.0.1",
    "request": "^2.78.0",
    "request-promise": "^4.1.1",
    "source-map-support": "^0.4.6"
  }
}